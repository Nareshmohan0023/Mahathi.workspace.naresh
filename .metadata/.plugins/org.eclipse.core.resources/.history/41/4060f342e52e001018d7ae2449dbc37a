package com.management.utils;

import java.util.Scanner;

public class InputUtil {
	
	Scanner sc=new Scanner(System.in);
	
	public String getStringInput(String prompt) {
		System.out.println(prompt);
		try {
			String input=sc.next();
		}catch (Exception e) {
			System.err.println();
		}
		return ;
		
	}

}
