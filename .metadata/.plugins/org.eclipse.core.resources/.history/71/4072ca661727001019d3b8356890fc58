package tasks;

public class Deamon_Thraed {
	public static void main(String[] args) {
		  
		Thread t1=new Thread(()->{
			for(int i=0;i<=10;i++) {
				System.out.println(i+":t1");
			}
		});
		Thread t2=new Thread(()->{
			for(int i=0;i<=50;i++) {
				System.out.println(i+":daemon");
			}
		});
	}

}
