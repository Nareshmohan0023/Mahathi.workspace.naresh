package com.entity;

public class Customer implements Runnable {

	private String Name;

	public Customer(String name) {
		super();
		Name = name;
	}

	public String getName() {
		return Name;
	}

	public void setName(String name) {
		Name = name;
	}

	@Override
	public void run() {
		System.out.println("Hi, I am "+this.getName()+"I like To order Some Food");
		
	}

}
